{
  "extends": "@zcloak/dev/config/tsconfig.json",
  "compilerOptions": {
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "composite": false,
    "jsx": "react-jsx",
    "paths": {
      "@zcloak/login-providers": ["login-providers/src"],
      "@zcloak/login-providers/*": ["login-providers/src/*"],
      "@zcloak/login-rpc": ["login-rpc/src"],
      "@zcloak/login-rpc/*": ["login-rpc/src/*"],
    },
    "skipLibCheck": true
  }
}
